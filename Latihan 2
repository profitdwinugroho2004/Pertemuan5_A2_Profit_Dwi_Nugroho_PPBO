class Animal:
    def speak(self):
        pass

class Snake(Animal):
    def speak(self):
        return "Sssttt!"

class Cat(Animal):
    def speak(self):
        return "Meowww!"

def animal_sound(animal):
    return animal.speak()

# Membuat objek dari kelas Dog dan Cat
snake = Snake()
cat = Cat()

# Memanggil metode speak menggunakan polimorfisme
print("Snake says:", animal_sound(snake))
print("Cat says:", animal_sound(cat))
